<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="izi_marketplace.form_wiz_recompute_mpxid" model="ir.ui.view">
            <field name="name">Wizard Recompute Marketplace External ID Form View</field>
            <field name="model">wiz.recompute_mpxid</field>
            <field name="arch" type="xml">
                <form string="Wizard Recompute Marketplace External ID">
                    <sheet>
                        <group>
                            <field name="model_ids" widget="many2many_tags" force_save="1"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_recompute" type="object" string="Recompute" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>